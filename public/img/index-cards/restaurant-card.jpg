<svg width="300" height="200" viewBox="0 0 300 200" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="200" fill="#FFE4B5"/>
  
  <!-- Restaurant building -->
  <rect x="50" y="100" width="200" height="80" fill="#8B4513" stroke="#654321" stroke-width="2"/>
  
  <!-- Roof -->
  <path d="M30 100 L150 60 L270 100 L250 100 L150 80 L50 100 Z" fill="#A0522D"/>
  
  <!-- Windows -->
  <rect x="70" y="120" width="30" height="40" fill="#87CEEB" stroke="#000" stroke-width="1"/>
  <rect x="120" y="120" width="30" height="40" fill="#87CEEB" stroke="#000" stroke-width="1"/>
  <rect x="170" y="120" width="30" height="40" fill="#87CEEB" stroke="#000" stroke-width="1"/>
  
  <!-- Door -->
  <rect x="140" y="140" width="20" height="40" fill="#654321" stroke="#000" stroke-width="1"/>
  
  <!-- Sign -->
  <rect x="100" y="70" width="100" height="20" fill="#FFD700" stroke="#000" stroke-width="1"/>
  <text x="150" y="83" text-anchor="middle" font-family="Arial, sans-serif" font-size="12" font-weight="bold" fill="#000">
    LOCAL DINING
  </text>
  
  <!-- Food icons -->
  <circle cx="80" cy="50" r="8" fill="#FF6347"/>
  <circle cx="220" cy="50" r="8" fill="#32CD32"/>
  <circle cx="150" cy="30" r="8" fill="#FFD700"/>
  
  <!-- Palm tree -->
  <g transform="translate(250, 120)">
    <rect x="0" y="0" width="4" height="30" fill="#8B4513"/>
    <path d="M2 0 Q-10 -15 -5 0 Q-10 15 2 0" fill="#228B22"/>
    <path d="M2 0 Q10 -15 5 0 Q10 15 2 0" fill="#228B22"/>
  </g>
</svg> 